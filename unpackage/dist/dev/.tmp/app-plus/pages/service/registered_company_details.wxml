<view class="service-body"><view class="service-title">{{service_name}}</view><view class="title">{{show_list[0].name}}</view><view><uni-grid vue-id="1" column="{{3}}" highlight="{{true}}" bind:__l="__l" vue-slots="{{['default']}}"><block wx:for="{{show_list[0].detail}}" wx:for-item="item" wx:for-index="index" wx:key="index"><uni-grid-item vue-id="{{('2-'+index)+','+('1')}}" bind:__l="__l" vue-slots="{{['default']}}"><image class="image" src="{{item.url}}" mode="aspectFill"></image><text class="text">{{item.text}}</text></uni-grid-item></block></uni-grid></view><view class="text">{{''+show_list[0].description}}</view><view class="title">{{show_list[1].name}}</view><block wx:for="{{show_list[1].detail}}" wx:for-item="info" wx:for-index="index" wx:key="index"><view class="text">{{info}}</view></block><view class="title">{{show_list[2].name}}</view><block wx:for="{{show_list[2].detail}}" wx:for-item="info" wx:for-index="index" wx:key="index"><view class="text">{{info}}</view></block><uni-fab vue-id="3" pattern="{{fab_para.pattern}}" content="{{fab_para.content}}" horizontal="{{fab_para.horizontal}}" vertical="{{fab_para.vertical}}" direction="{{fab_para.direction}}" data-event-opts="{{[['^trigger',[['fab_trigger']]]]}}" bind:trigger="__e" bind:__l="__l"></uni-fab></view>