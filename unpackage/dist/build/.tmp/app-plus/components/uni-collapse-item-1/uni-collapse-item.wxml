<view class="{{['uni-collapse-cell',[(disabled)?'uni-collapse-cell--disabled':'',(isOpen)?'uni-collapse-cell--open':'']]}}" hover-class="{{disabled?'':'uni-collapse-cell--hover'}}"><view data-event-opts="{{[['tap',[['onClick',['$event']]]]]}}" class="uni-collapse-cell__title" bindtap="__e"><block wx:if="{{thumb}}"><view class="uni-collapse-cell__title-extra"><image class="uni-collapse-cell__title-img" src="{{thumb}}"></image></view></block><view class="uni-collapse-cell__title-inner"><view class="uni-collapse-cell__title-text row"><view class="col1">{{title}}</view><view class="col1"><view class="text-gray">余额 :<text class="td">{{begin}}</text></view></view></view></view><view class="{{['uni-collapse-cell__title-arrow',(isOpen)?'uni-active':'',(showAnimation===true)?'uni-collapse-cell--animation':'']}}"></view></view><view class="{{['uni-collapse-cell__content',(showAnimation===true)?'uni-collapse-cell--animation':'']}}" style="{{'height:'+(isOpen?height:'0px')+';'}}"><view class="view" id="{{elId}}"><slot></slot></view></view></view>